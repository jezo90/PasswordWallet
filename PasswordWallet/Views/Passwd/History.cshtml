@model PasswordWallet.Models.AppViewModel

<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-info">
            History List
        </h2>
    </div>
    <div class="col-3 offset-3">
        <a asp-action="Index" asp-controller="Passwd" class="btn btn-info form-control text-white">
            Back to list
        </a>
    </div>
    <span class="text-danger">@Model.Error</span> <br />
    <div class="col-12 border p-3">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Old Password</th>
                    <th>New Password</th>
                    <th>Time</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/historyList.js"></script>
}
