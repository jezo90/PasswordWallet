@model PasswordWallet.Models.AppViewModel

<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-info">
            Passwords List
        </h2>
    </div>
    <div class="col-3 offset-3">
        <a asp-action="Upsert" asp-controller="Passwd" class="btn btn-info form-control text-white">
            Add new data
        </a>
    </div>
    <div class="col-12 border p-3">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Login</th>
                    <th>Password</th>
                    <th>Web</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div> 

@section Scripts{ 
<script src="~/js/passwdList.js"></script>
}